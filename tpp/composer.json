{
    "name": "tokenio/tpp",
    "description": "SDK for interacting with the Token Core Services",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Token.io",
            "homepage": "https://token.io"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-grpc": "*",
        "paragonie/sodium_compat": "1.7.0",
        "paragonie/random_compat": ">=2",
        "stephenhill/base58": "~1.0",
        "grpc/grpc": "^1.20.0",
        "google/protobuf": "^3.6.1",
        "jms/serializer": "1.7.1",
        "composer/composer": "^1.9",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Tokenio\\":"./../core/src/",
            "Test\\": "tests/",
            "Tokenio\\Tpp\\": "src/",
            "Google\\": "./../core/Proto/Google",
            "GPBMetadata\\": "./../core/Proto/GPBMetadata",
            "Io\\": "./../core/Proto/Io/"
        },
        "classmap": ["./../core/src/"]
    },
    "suggest": {
        "ext-libsodium": "PHP < 7.0: Better performance, password hashing (Argon2i), secure memory management (memzero), and better security.",
        "ext-sodium": "PHP >= 7.0: Better performance, password hashing (Argon2i), secure memory management (memzero), and better security."
    }
}